{
  "expo": {
    "name": "Wizzmo",
    "slug": "wizzmo-college-advice",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wizzmo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/ipad_load.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF4DB8"
    },
    "backgroundColor": "#FF4DB8",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wizzmo.app",
      "buildNumber": "32",
      "usesAppleSignIn": true,
      "splash": {
        "image": "./assets/images/ipod_load.png",
        "tabletImage": "./assets/images/ipad_load.png",
        "resizeMode": "contain",
        "backgroundColor": "#FF4DB8"
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.com.wizzmo.app"
        ],
        "aps-environment": "production"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSUserNotificationAlertStyle": "alert",
        "UIFileSharingEnabled": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "google-oauth-wizzmo",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.753037060504-s9lvudr16n4cbpssfq7dbak0lt5hr7iv"
            ]
          }
        ]
      },
      "googleServicesFile": "./Client ID for iOS Wismo.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "package": "com.wizzmo.app",
      "versionCode": 32,
      "permissions": [
        "INTERNET",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "com.android.vending.BILLING"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "jsEngine": "jsc",
    "platforms": [
      "ios",
      "android"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "051ac1ce-5198-481f-9ab0-406c876a2e9c"
      }
    },
    "runtimeVersion": "1.0.0",
    "owner": "reaganroo22"
  }
}
